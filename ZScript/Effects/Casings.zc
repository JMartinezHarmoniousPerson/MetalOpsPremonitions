class BaseCasing : Actor //Base class to inherit
{
    Default
    {
        Height 2;
        Radius 2;
        Speed 8;
        Scale 0.1;
        BounceType "Doom";
        -NOGRAVITY;
        +WINDTHRUST;
        +CLIENTSIDEONLY;
        +MOVEWITHSECTOR;
        +MISSILE;
        +NOBLOCKMAP;
        -DROPOFF;
        +NOTELEPORT;
        +FORCEXYBILLBOARD;
        +NOTDMATCH;
        +GHOST;
        -DONTSPLASH;
        -CANBOUNCEWATER;
        +FLOORCLIP;
        +THRUACTORS;
        +ROLLSPRITE;
        Mass 4;
        BounceSound "";
		BounceCount 3;
    }
}

class PistolCasing: BaseCasing
{	
	Default
   {
		Scale 0.065;
		BounceSound "casings/lightbrass";
	}
   States
   {
   Spawn:
	  Spawn:
			TNT1 A 0;
			CS70 ABCDEFGHABCDEFGHABCDEFGHABCDEFGHABCDEFGHABCDEFGH 2 {
				A_SpawnItemEx("BulletSmoke",frandom(0.3,0.2),random(0,-1.0),0,0,0,frandom(0.5,0.1),0,SXF_CLIENTSIDE,0);
				A_SetRoll(roll+45);
			}
			Goto Death;


	   Death:
		   C4S1 I 0;
		   C4S1 I 0 A_SpawnItemEx("RestingBulletSmoke");
		   TNT1 A 0 A_SetRoll(0);
		   TNT1 A 0 A_Jump(256,"Case1","Case2","Case3","Case4","Case5","Case6","Case7","Case8");
			Case1:
				CS70 K 1;
				Goto CompletelyRest;
			Case2:
				CS70 L 1;
				Goto CompletelyRest;
			Case3:
				CS70 M 1;
				Goto CompletelyRest;
			Case4:
				CS70 N 1;
				Goto CompletelyRest;
			Case5:
				CS70 O 1;
				Goto CompletelyRest;
			Case6:
				CS70 P 1;
				Goto CompletelyRest;
			Case7:
				CS70 Q 1;
				Goto CompletelyRest;

    CompletelyRest:
      #### # 350;
	  #### # 1 A_FadeOut(0.01);
      Wait;
   }
}

