class ARMagazine : BaseCasing
{
	Default
   {
		Scale 0.25;
		+ROLLSPRITE;
		BounceSound "casings/largemag";
   }
   States
   {
    Spawn:
	  TNT1 A 0 ;
      CLP1 A 3 A_SetRoll(roll+36, SPF_INTERPOLATE);
      Loop;

   Death:
      C4S1 I 0 A_SetRoll(0);
      LCPJ A 0 A_Jump(256,"Rest1","Rest2");
      Goto Rest1;
    Rest1:
      CLP1 B 1;
       Goto CompletelyRest;
    Rest2:
	  TNT1 A 0 {bXFLIP = TRUE;}
      CLP1 B 1;
      Goto CompletelyRest;

    CompletelyRest:
			#### # 350;
			#### # 1 A_FadeOut(0.01);
			Wait;
   }
}

Class MinigunBeltPiece : BaseCasing
{
   Default
   {Height 2;
   Radius 2;
   Speed 8;
   Scale .12;
   SeeSound "casings/heavybrass";
   }
   States
   {
    Spawn:
    Spawn1:
		LMLN D 0 A_Jump(128,2);
		LMLN C 0;
	  Exist:
      "####" "#" 3 {
		A_SetRoll(roll+36);
		A_JumpIf(waterlevel > 1, "Splash");
		}
      Loop;

   Death:
	   "####" "#" 0 A_SETROLL(0);
      "####" "#" 0 A_StopSound(CHAN_AUTO);
	  "####" "#" 0 A_StartSound("decasebounce");  
    CompletelyRest:
		#### # 350;
		#### # 1 A_FadeOut(0.01);
		Wait;
	
		
     Disappear:
     TNT1 A 1;
     Stop;
	 
	Splash:
	TNT1 A 0;
	Stop;
   }
}

